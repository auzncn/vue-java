<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.dao.GoodsDescDao">


    <select id="getByGoodsId" resultType="com.example.demo.entity.GoodsDesc" parameterType="long">
        select title,content from goods_desc where goods_id = #{goodsId}
    </select>
</mapper>
